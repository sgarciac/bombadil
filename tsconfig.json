{
  "compilerOptions": {
    "target":"es5",
    "declaration": false,
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "module": "commonjs",
    "outDir": "dist",
    "sourceMap": true,
    "watch": true
  },
  "include": ["*.ts"],
  "exclude": [
    "node_modules"
  ],
  "compileOnSave": false,
  "buildOnSave": true,
  "atom": { "rewriteTsconfig": false }
}
